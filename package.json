{
  "name": "@adobe/aem-boilerplate-commerce",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project for Adobe Commerce on Edge Delivery Services",
  "scripts": {
    "install:dropins": "node build.mjs && node postinstall.js",
    "postinstall": "npm run install:dropins",
    "postupdate": "npm run install:dropins",
    "lint:js": "eslint . --ext .json,.js,.mjs",
    "lint:css": "stylelint \"blocks/**/*.css\" \"styles/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "aem up",
    "build:json": "npm-run-all -p build:json:models build:json:definitions build:json:filters",
    "build:json:models": "merge-json-cli -i \"models/_component-models.json\" -o \"component-models.json\"",
    "build:json:definitions": "merge-json-cli -i \"models/_component-definition.json\" -o \"component-definition.json\"",
    "build:json:filters": "merge-json-cli -i \"models/_component-filters.json\" -o \"component-filters.json\"",
    "watch:json": "node watch-json.js",
    "prepare": "husky",
    "s": "aem up --url https://main--nasm--sprucetechnology.aem.page",
    "st": "aem up --url https://stage--nasm-stage--sprucetechnology.aem.page",
    "createblock": "node tooling/create-block.js",
    "dev:dialog": "node server/dev-config-server.js",
    "predev:dialog": "npm run build:json",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:blocks": "vitest blocks",
    "test:scripts": "vitest tests/scripts",
    "test:utils": "vitest tests/utils",
    "test:ci": "vitest --run",
    "storybook": "storybook dev -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hlxsites/aem-boilerplate-commerce.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/hlxsites/aem-boilerplate-commerce/issues"
  },
  "homepage": "https://github.com/hlxsites/aem-boilerplate-commerce#readme",
  "devDependencies": {
    "@adobe/aem-cli": "^16.5.8",
    "@babel/eslint-parser": "7.27.0",
    "@dropins/build-tools": "0.2.0",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/html-vite": "^9.1.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-storybook": "9.1.3",
    "eslint-plugin-xwalk": "github:adobe-rnd/eslint-plugin-xwalk#v0.1.3",
    "husky": "9.1.1",
    "merge-json-cli": "1.0.4",
    "npm-run-all": "4.1.5",
    "pino": "^9.7.0",
    "playwright": "^1.55.0",
    "storybook": "^9.1.3",
    "stylelint": "16.17.0",
    "stylelint-config-standard": "37.0.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@adobe/magento-storefront-event-collector": "^1.8.0",
    "@adobe/magento-storefront-events-sdk": "^1.8.0",
    "@dropins/storefront-account": "~1.0.9",
    "@dropins/storefront-auth": "~2.1.0",
    "@dropins/storefront-cart": "1.5.1-beta1",
    "@dropins/storefront-checkout": "2.0.0",
    "@dropins/storefront-order": "~1.3.0",
    "@dropins/storefront-payment-services": "~1.0.1",
    "@dropins/storefront-pdp": "^1.1.0",
    "@dropins/storefront-product-discovery": "^2.0.0",
    "@dropins/tools": "1.5.0-alpha7000",
    "@magento/storefront-search-as-you-type": "^1.0.4",
    "@tailwindcss/cli": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.1.0",
    "enquirer": "^2.4.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "minimist": "^1.2.8",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "20.19.3"
  }
}
